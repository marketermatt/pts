div.rotatingtweets{
    overflow-y: hidden !important;
    min-height: 165px !important;
}

div.rotatingtweet{
    padding: 10px 15px;
    background: #141414;
    color: #fff;
    overflow-y: visible !important;
}

div.rotatingtweet:before{
    position: absolute;
    display: inline-block;
    width: 0px;
    height: 0px;
    border-left: 20px solid transparent;
    border-right: 20px solid transparent;
    border-top: 20px solid #141414;
    left: 42%;
    bottom: -15px;
    content: '';
}

div.rotatingtweet:after{
    position: absolute;
    bottom: -40px;
    left: 46%;
    content: '\f099';
    font-family: FontAwesome;
    font-size: 20px;
}

div.rotatingtweet p{
    color: #fff;
}

